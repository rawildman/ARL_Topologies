<representation type="heavisidemesh2d" tag="rep1"/>
<heavisidemesh2d tag="rep1">
	<!--Automatically mesh 2d region specified below-->
	<!--First polygon in list is the outer boundary and remaining are holes-->
	<file_format>polygon</file_format>
  <polygon_list>
    <polygon_2>
      <point_2 x="0." y="0."/>
      <point_2 x="1." y="0."/>
      <point_2 x="0." y="1."/>
    </polygon_2>
    <polygon_2>
      <point_2 x="0.1" y="0.1"/>
      <point_2 x="0.2" y="0.1"/>
      <point_2 x="0.2" y="0.2"/>
      <point_2 x="0.1" y="0.2"/>
    </polygon_2>
  </polygon_list>
	<minimum_density>1.e-3</minimum_density>
  <penalty_power>2.</penalty_power>
	<threshold>0.75</threshold>
  <heaviside_beta>1.e-12</heaviside_beta>
  <filter_radius>2.2</filter_radius>
	<mesh_parameters dim="2">
    <tri_mesh_edge_size>0.1</tri_mesh_edge_size>
    <tri_mesh_edge_angle>15.</tri_mesh_edge_angle>
  </mesh_parameters>
</heavisidemesh2d>
